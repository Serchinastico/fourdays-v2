{
  "expo": {
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F2F7F5",
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.fourdays.app"
    },
    "extra": {
      "eas": {
        "projectId": "434c34b4-f07c-4002-9290-880999122090"
      }
    },
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fourdays.app"
    },
    "name": "fourdays",
    "orientation": "portrait",
    "owner": "serchinastico",
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your camera to let you pick your online avatar.",
          "photosPermission": "The app accesses your photos to add new ingredients to your food library."
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#52BD76"
              }
            }
          }
        }
      ],
      [
        "./plugins/date-time-picker-style.js"
      ]
    ],
    "slug": "four-days",
    "splash": {
      "backgroundColor": "#F2F7F5",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "version": "1.0.24",
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
